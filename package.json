{
  "name": "toy-monorepo",
  "version": "1.0.0",
  "description": "토이프로젝트 모노레포",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "dev:backend": "nx serve backend",
    "dev:frontend": "nx dev frontend",
    "build": "nx run-many --target=build --projects=backend,frontend --parallel",
    "build:backend": "nx build backend",
    "build:frontend": "nx build frontend",
    "test": "nx run-many --target=test --all",
    "test:backend": "nx test backend",
    "test:frontend": "nx test frontend",
    "lint": "nx run-many --target=lint --all",
    "lint:backend": "nx lint backend",
    "lint:frontend": "nx lint frontend",
    "graph": "nx graph",
    "affected:graph": "nx affected:graph",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/devkit": "22.3.3",
    "@nx/eslint": "22.3.3",
    "@nx/eslint-plugin": "22.3.3",
    "@nx/jest": "22.3.3",
    "@nx/js": "22.3.3",
    "@nx/nest": "^22.3.3",
    "@nx/next": "^22.3.3",
    "@nx/node": "^22.3.3",
    "@nx/playwright": "22.3.3",
    "@nx/react": "^22.3.3",
    "@nx/web": "22.3.3",
    "@nx/webpack": "22.3.3",
    "@nx/workspace": "^22.3.3",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.8.0",
